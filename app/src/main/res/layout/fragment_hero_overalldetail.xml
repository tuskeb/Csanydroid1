<TableLayout
    android:id="@+id/hero_detail_containerrr"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="5dp"
    android:stretchColumns="0"
    tools:context="hu.csany_zeg.one.csanydroid1.HeroDetailActivity"
    tools:ignore="MergeRootFrame">

    <TableRow style="@style/hero_overalldetail_row">
        <TextView
            android:text="@string/statistics_number_of_heros"
            style="@style/hero_overalldetail_name"/>
        <TextView
            android:id="@+id/number_of_heros"
            style="@style/hero_overalldetail_value"/>
    </TableRow>

</TableLayout>
